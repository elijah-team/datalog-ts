{
  "name": "datalog-ts",
  "version": "0.5.9",
  "scripts": {
    "test": "ts-node allTests.ts",
    "test-stay-alive": "ts-node allTests.ts --stay-alive",
    "test-write-results": "ts-node allTests.ts --write-results",
    "repl": "ts-node apps/cmd/repl.ts",
    "build-dist": "tsc --jsx react --outDir dist --declaration --esModuleInterop apps/fiddle/main.tsx && tsc --outDir dist apps/cmd/repl.ts",
    "build-interp-bundle": "esbuild --bundle --outfile=dist/index.mjs --format=esm --target=es6 core/interpreter.ts",
    "build-fp-ui": "esbuild apps/fp/ui/main.tsx --bundle --outfile=apps/fp/ui/bundle.js '--define:process.env.NODE_ENV=\"development\"' --loader:.dl=text --loader:.css=text --sourcemap",
    "serve-fp-ui": "cd apps/fp/ui && http-server -c-1",
    "build-fiddle": "esbuild apps/fiddle/main.tsx --bundle --outfile=apps/fiddle/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --loader:.dl=text --sourcemap",
    "serve-fiddle": "cd apps/fiddle/public && http-server -c-1",
    "build-notebook-ui": "esbuild apps/notebook/main.tsx --bundle --outfile=apps/notebook/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --sourcemap",
    "serve-notebook-ui": "cd apps/notebook/public && http-server -c-1",
    "build-parserlib-playground": "esbuild apps/parserlib/playground/main.tsx --bundle --outfile=apps/parserlib/playground/public/bundle.js '--define:process.env.NODE_ENV=\"development\"' --sourcemap",
    "serve-parserlib-playground": "cd apps/parserlib/playground/public && http-server -c-1",
    "cloc": "cloc --vcs=git --exclude-dir=dist"
  },
  "devDependencies": {
    "@jaegertracing/plexus": "^0.1.0",
    "@types/diff": "^4.0.2",
    "@types/node": "^13.13.0",
    "@types/parsimmon": "^1.10.1",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "diff": "^4.0.2",
    "esbuild": "^0.5.18",
    "http-server": "^0.12.3",
    "prettier": "^2.0.4"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "parsimmon": "^1.13.0",
    "prettier-printer": "^1.1.4",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.19.1",
    "react-router-dom": "^5.2.0",
    "react-simple-code-editor": "https://github.com/vilterp/react-simple-code-editor#a1f205b84348964fba02163d1e52e908613d484b",
    "react-textarea-autosize": "^8.2.0",
    "react-use-localstorage": "^3.4.1",
    "simple-markdown": "^0.7.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "use-hash-param": "^0.2.3",
    "use-http": "^1.0.13"
  },
  "bin": {
    "datalog-ts": "dist/cmd/repl.js"
  },
  "files": [
    "dist"
  ]
}
